{
  "version": "0.2.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "example": "parcel example/index.html example/**/*",
    "clean": "rm -rf .parcel-cache coverage dist"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "jspsych-wrapper",
  "author": "Henry Burgess",
  "module": "dist/jspsych-wrapper.esm.js",
  "size-limit": [
    {
      "path": "dist/jspsych-wrapper.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/jspsych-wrapper.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.5",
    "@types/jquery": "^3.5.13",
    "husky": "^7.0.4",
    "size-limit": "^7.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@types/d3": "^7.1.0",
    "consola": "^2.15.3",
    "d3-random": "^3.0.1",
    "jquery": "^3.6.0",
    "jspsych": "^6.0.0",
    "parcel": "^2.2.1"
  }
}
